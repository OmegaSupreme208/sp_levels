"UnlitTwoTexture"
{
	"$baseTexture" "models/holo_screens/tv_imc_saver_logo_col"
	"$texture2" "models/holo_screens/tv_imc_saver_logo_rotate_col"
//	"$additive" 1
	"$translucent" 1


	$allowoverbright 1
	"$color2" "[1.3 1.3 1.3]"
	
	
	"$rotatespeed" 1
	
	"Proxies"
	{
		//chevron flicker
		"UniformNoise"
		{
			"minVal" .95
			"maxVal" 1
			"resultVar" "$alpha"
		}
		
			//rotate the basetexture using a variable
		"LinearRamp"
		{
			"rate" 30
			"resultVar" "$rotatespeed"
		}
		
			"TextureTransform"
		{
			"rotateVar" "$rotatespeed"
			"resultvar" "$texture2transform"
		}
	}
}