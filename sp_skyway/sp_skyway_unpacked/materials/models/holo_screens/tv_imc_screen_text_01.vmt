"UnlitTwoTexture"
{
	"$baseTexture" "models/holo_screens/tv_imc_screen_text_back_col"
	"$texture2" "models/holo_screens/tv_imc_screen_text_01_col"
//	"$additive" 1
	"$translucent" 1


	$allowoverbright 1
	"$color2" "[1.3 1.3 1.3]"

	"$translatespeed" "[1 1]"
	
	"Proxies"
	{
		//chevron flicker
		"UniformNoise"
		{
			"minVal" .9
			"maxVal" 1
			"resultVar" "$alpha"
		}		
	
		"TextureScroll"
		{
			"textureScrollRate" .2
			"textureScrollAngle" -90
			"textureScrollVar" "$translatespeed"
		}		

				"TextureTransform"
		{
			"translateVar" "$translatespeed"
			"resultvar" "$texture2transform"			
		}
		
		
	}
}