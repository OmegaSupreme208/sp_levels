	"$vertexalpha" 1
	"$model" "1"
	"$translucent" 1
	"$nocull" "1"
	"$color" "[1 1.5 2]"
	//"$color2" "[.2 .25 .75]"

	//"$godray" "1"
	//"$godrayMinFadeDist" "50"
	//"$godrayMaxFadeDist" "100"


	"$basetexture" "effects\caustic_water_01"
	//"$basetexture" "effects\caustic_water_01"
	//"$basetexture" "effects\cloudlayer"
	"$Texture2" "effects\caustic_water_01"



//	"$color" "[0.43 0.43 0.43]"
//	"$color2" "[0.72 2 0.71]"

	"Proxies"
	{
		"TextureScroll"
		{
			"texturescrollvar" "$basetexturetransform"
			"texturescrollrate" 3
			"texturescrollangle" 90
		}
		"TextureScroll"
		{
			"texturescrollvar" "$texture2transform"
			"texturescrollrate" 1
			"texturescrollangle" -40
		}

		//flicker
		"UniformNoise"
		{
			"minVal" 0.0
			"maxVal" .5
			"resultVar" "$alpha"
		}
	}
}
