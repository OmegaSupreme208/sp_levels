"UnlitTwoTexture"
{
//	"$nocull" "1"	
	"$nodecal" "1"
	"$translucent"	1
	"$additive" "1"
	"$no_fullbright" 1
	"$vertexcolor" 1
	"$vertcoloristint" "1"
	"$vertexalpha" 1
	"$model" "1"
	"%compileNonsolid"         1
	$allowoverbright 1

	$fresnel 1
	$fresnelsharpness1 1
	$fresnelouterstrength1 1
	$fresnelinnerstrength1 2


	"$basetexture" "models\holo_screens\holo_map_grid"
	"$Texture2" "dev\noise_post"

	"$color2" "[3 2 .4]"
//	"$color2" "[2 1.26 0.32]"
//	"$color2" "[4 2.52 0.64]"
		
	"$t1scroll"	0

	"$t2offset" 0
	"$t2scale" 1
	"$t2rot" 0
	"$scroll" "[0 1]"
	
	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$texture2"
			"animatedtextureframenumvar" "$frame2"
			"animatedtextureframerate" 30 
		}
		//Horizontal jitter speed 
		"UniformNoise"
		{
			"minVal" -1
			"maxVal" 1
			"resultVar" "$scroll[0]"
		}
		//Vertical jitter speed 
		"UniformNoise"
		{
			"minVal" -1
			"maxVal" 1
			"resultVar" "$scroll[1]"
		}
		//Move the 2nd texture using horiz and vert speed
		"TextureTransform"
		{
			"translateVar" "$scroll"
			"scaleVar" "$t2scale"
			"rotateVar" "$t2rot"
			"resultvar" "$texture2transform"
		}
		//Scroll the base texture
		"TextureScroll"
		{
			"texturescrollvar" $basetexturetransform
			"texturescrollrate" $t1scroll
			"texturescrollangle" 90
		}
	}
}